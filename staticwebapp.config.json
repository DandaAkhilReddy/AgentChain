{
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": ["anonymous"]
        },
        {
            "route": "/dapp/*",
            "allowedRoles": ["anonymous"]
        },
        {
            "route": "/*",
            "serve": "/website/index.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "/website/index.html",
        "exclude": ["/api/*", "/dapp/*", "/*.{css,scss,sass,js,ts,tsx,jsx,json,png,gif,jpg,jpeg,svg,ico,woff,woff2,ttf,eot}"]
    },
    "mimeTypes": {
        ".json": "application/json",
        ".js": "application/javascript",
        ".css": "text/css",
        ".html": "text/html",
        ".png": "image/png",
        ".jpg": "image/jpeg",
        ".jpeg": "image/jpeg",
        ".gif": "image/gif",
        ".svg": "image/svg+xml",
        ".ico": "image/x-icon",
        ".woff": "font/woff",
        ".woff2": "font/woff2",
        ".ttf": "font/ttf",
        ".eot": "application/vnd.ms-fontobject"
    },
    "globalHeaders": {
        "Cache-Control": "public, max-age=31536000, immutable"
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/website/index.html",
            "statusCode": 200
        }
    }
}